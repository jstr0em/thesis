  keywords = {Biodegradation,Convection-diffusion,Explicit analytic solution,Model,Nonhomogeneous surface boundary conditions,Soil contamination,Vapor intrusion,Volatilization},
  language = {en},
  note = {00014},
  number = {1}
}

