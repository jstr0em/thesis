@article{pennell_field_2016,
  title = {Field Data and Numerical Modeling: {{A}} Multiple Lines of Evidence Approach for Assessing Vapor Intrusion Exposure Risks},
  shorttitle = {Field Data and Numerical Modeling},
  author = {Pennell, Kelly G. and Scammell, Madeleine K. and McClean, Michael D. and Suuberg, Eric M. and Moradi, Ali and Roghani, Mohammadyousef and Ames, Jennifer and Friguglietti, Leigh and Indeglia, Paul A. and Shen, Rui and Yao, Yijun and {Heiger-Bernays}, Wendy J.},
