@article{nazaroff_radon_1985,
  title = {Radon Transport into a Detached One-Story House with a Basement},
  author = {Nazaroff, W. W. and Feustel, H. and Nero, A. V. and Revzan, K. L. and Grimsrud, D. T. and Essling, M. A. and Toohey, R. E.},
  year = {1985},
  month = jan,
  volume = {19},
